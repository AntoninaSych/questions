<%=form_with(model: @question, data: { turbo: false }) do |f| %>
    <div>
        <%= f.label :title %>
        <%= f.text_field :title, required:true, minlength:2, placeholder: "Titile" %>
    <div>
    <div>
        <%= f.label :body %>
        <%= f.text_area :body, required:true, minlength:2, placeholder: "Body" %>
    <div>
 <%= f.submit "Submit Question" %>
<% end %>